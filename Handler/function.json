{
    "scriptFile": "__init__.py",
    "bindings": [
      {
        "route": "{*route}",      
        "authLevel": "function",
        "type": "httpTrigger",
        "direction": "in",
        "name": "req",
        "methods": [
          "get",
          "post",
          "options"
        ]
      },
      {
        "type": "http",
        "direction": "out",
        "name": "$return"
      }
    ]
  }